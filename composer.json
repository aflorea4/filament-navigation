{
    "name": "aflorea4/filament-navigation",
    "description": "Build structured navigation menus in Filament.",
    "keywords": [
        "aflorea4",
        "laravel",
        "filament-navigation"
    ],
    "homepage": "https://github.com/aflorea4/filament-navigation",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandru Florea",
            "email": "alex@aflorea.dev",
            "role": "Developer"
        },
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "role": "Original Author"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^2.9",
        "orchestra/canvas": "^9.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ray": "^1.36"
    },
    "autoload": {
        "psr-4": {
            "Aflorea4\\FilamentNavigation\\": "src",
            "Aflorea4\\FilamentNavigation\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aflorea4\\FilamentNavigation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aflorea4\\FilamentNavigation\\FilamentNavigationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}